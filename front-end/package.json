  "scripts": {
    "start": "webpack serve --mode development --hot --open --config ./webpack/webpack.dev.config.ts",
    "build": "webpack --mode production --config ./webpack/webpack.prod.config.ts",
    "test": "jest",
    "test:ci": "jest --runInBand --coverage --watchAll=false --coverageReporters=cobertura lcov --reporters=default --reporters=jest-junit --transformIgnorePatterns=\"node_modules/(?!axios)\"",
    "test:coverage": "jest --coverage --watchAll=false",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx} ./cypress/**/*.{js,jsx,ts,tsx} --max-warnings=0",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "cy:open": "cypress open",
    "cy:run": "cypress run --record --key 2d777294-bb39-47b7-bd5c-4556b89d58da",
    "cy:verify": "cypress verify",
    "cy:ci": "start-server-and-test 'serve -s build' http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
    ]
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "outputDirectory": "coverage",
    "outputName": "test-output-junit.xml"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:storybook/recommended"
    ]
  }
}
